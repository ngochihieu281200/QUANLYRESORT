        <script type="text/javascript">
            Sys.WebForms.PageRequestManager.getInstance().add_beginRequest(BeginRequestHandler);
            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandler);
            function BeginRequestHandler(sender, args) {
                var pbControl = args.get_postBackElement();
                if (pbControl.id.indexOf('_qc_') > -1)
                    ActivateLoadingDiv('visible', 'QuestLoadingImageDiv');
                else if (pbControl.id.indexOf('_tc_') > -1)
                    ActivateLoadingDiv('visible', 'TransLoadingImageDiv');
                else if (pbControl.id.indexOf('_pc_') > -1)
                    ActivateLoadingDiv('visible', 'PaymentLoadingImageDiv');
                else if (pbControl.id.indexOf('ClearGuest') > -1)
                    ActivateLoadingDiv('visible', 'CustomerLoadingImageDiv');
                else if (pbControl.id.indexOf('_mc_') > -1)
                    ActivateLoadingDiv('visible', 'LoyaltyLoadingImageDiv');
                else if (pbControl.id.indexOf('_ga_') > -1)
                    ActivateLoadingDiv('visible', 'AddressLoadingImageDiv');
                else if (pbControl.id.indexOf('_cc_') > -1)
                    ActivateLoadingDiv('visible', 'PaymentTypeLoadingImageDiv');
                else if (pbControl.id.indexOf('_rac_') > -1)
                    ActivateLoadingDiv('visible', 'RezAgentLoadingImageDiv');
                if (typeof (rezRequestBtn) != 'undefined')
                    document.getElementById(rezRequestBtn).disabled = true;
            }
            function EndRequestHandler(sender, args) {
                ActivateLoadingDiv('hidden', 'QuestLoadingImageDiv');
                ActivateLoadingDiv('hidden', 'TransLoadingImageDiv');
                ActivateLoadingDiv('hidden', 'PaymentLoadingImageDiv');
                ActivateLoadingDiv('hidden', 'CustomerLoadingImageDiv');
                ActivateLoadingDiv('hidden', 'LoyaltyLoadingImageDiv');
                ActivateLoadingDiv('hidden', 'AddressLoadingImageDiv');
                ActivateLoadingDiv('hidden', 'PaymentTypeLoadingImageDiv');
                ActivateLoadingDiv('hidden', 'RezAgentLoadingImageDiv');
                if (typeof (rezRequestBtn) != 'undefined')
                    document.getElementById(rezRequestBtn).disabled = false;
            }
            function ActivateLoadingDiv(visstring, elem) {
                var adiv = document.getElementById(elem);
                if (adiv) adiv.style.visibility = visstring;
